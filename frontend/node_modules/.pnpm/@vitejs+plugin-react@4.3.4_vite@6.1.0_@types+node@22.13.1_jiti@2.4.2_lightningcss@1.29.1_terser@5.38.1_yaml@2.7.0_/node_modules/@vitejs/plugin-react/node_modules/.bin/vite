#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/frontend/node_modules/.pnpm/vite@6.1.0_@types+node@22.13.1_jiti@2.4.2_lightningcss@1.29.1_terser@5.38.1_yaml@2.7.0/node_modules/vite/bin/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/frontend/node_modules/.pnpm/vite@6.1.0_@types+node@22.13.1_jiti@2.4.2_lightningcss@1.29.1_terser@5.38.1_yaml@2.7.0/node_modules/vite/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/frontend/node_modules/.pnpm/vite@6.1.0_@types+node@22.13.1_jiti@2.4.2_lightningcss@1.29.1_terser@5.38.1_yaml@2.7.0/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/frontend/node_modules/.pnpm/vite@6.1.0_@types+node@22.13.1_jiti@2.4.2_lightningcss@1.29.1_terser@5.38.1_yaml@2.7.0/node_modules/vite/bin/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/frontend/node_modules/.pnpm/vite@6.1.0_@types+node@22.13.1_jiti@2.4.2_lightningcss@1.29.1_terser@5.38.1_yaml@2.7.0/node_modules/vite/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/frontend/node_modules/.pnpm/vite@6.1.0_@types+node@22.13.1_jiti@2.4.2_lightningcss@1.29.1_terser@5.38.1_yaml@2.7.0/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@6.1.0_@types+node@22.13.1_jiti@2.4.2_lightningcss@1.29.1_terser@5.38.1_yaml@2.7.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@6.1.0_@types+node@22.13.1_jiti@2.4.2_lightningcss@1.29.1_terser@5.38.1_yaml@2.7.0/node_modules/vite/bin/vite.js" "$@"
fi
