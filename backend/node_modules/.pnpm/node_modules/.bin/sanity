else
  export NODE_PATH="/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/backend/node_modules/.pnpm/@sanity+cli@3.74.1_@types+babel__core@7.20.5_@types+node@22.13.1_@types+react@18.3.18_react@18.3.1_typescript@5.7.3/node_modules/@sanity/cli/bin/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/backend/node_modules/.pnpm/@sanity+cli@3.74.1_@types+babel__core@7.20.5_@types+node@22.13.1_@types+react@18.3.18_react@18.3.1_typescript@5.7.3/node_modules/@sanity/cli/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/backend/node_modules/.pnpm/@sanity+cli@3.74.1_@types+babel__core@7.20.5_@types+node@22.13.1_@types+react@18.3.18_react@18.3.1_typescript@5.7.3/node_modules/@sanity/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/backend/node_modules/.pnpm/@sanity+cli@3.74.1_@types+babel__core@7.20.5_@types+node@22.13.1_@types+react@18.3.18_react@18.3.1_typescript@5.7.3/node_modules:/home/rolf/Kode/Projects/Sanity/SanityTest/sanity-test/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
